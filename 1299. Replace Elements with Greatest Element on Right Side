------- brute force--------
class Solution {
public:
    vector<int> replaceElements(vector<int>& v) {
        vector<int> res;
        for(int i=0;i<v.size();i++)
        {
            int ans=-1;
            for(int j=i+1;j<v.size();j++)
                if(ans < v[j])
                    ans=v[j];
        res.push_back(ans);        
        }
        return res;
        
    }
};


---- ------
